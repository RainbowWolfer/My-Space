<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.rainbowwolfer.myspacedemo1">
	
	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name_space"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/Theme.MySpaceDemo1">
		<activity
			android:name="com.rainbowwolfer.myspacedemo1.ui.activities.user.LoginActivity"
			android:exported="false"
			android:windowSoftInputMode="adjustPan"/>
		<activity
			android:name="com.rainbowwolfer.myspacedemo1.ui.activities.welcome.WelcomeActivity"
			android:exported="true"
			android:theme="@style/Theme.MySpaceDemo1.NoActionBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity
			android:name="com.rainbowwolfer.myspacedemo1.ui.activities.main.MainActivity"
			android:exported="true"
			android:theme="@style/Theme.MySpaceDemo1.NoActionBar"
			android:windowSoftInputMode="adjustPan"/>
	</application>

</manifest>